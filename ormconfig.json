{
  "type": "sqlite",
  "database": "andela.sqlite",
  "name": "default",
  "synchronize": false,
  "logging": true,
  "migrationsTableName": "custom_migration_table",
  "entities": [
    "src/models/*.ts"
  ],
  "migrations": [
    "src/migrations/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migrations"
  }
}